# Thirdweb Configuration
# Get your secret key from https://thirdweb.com/dashboard/settings/api-keys
THIRDWEB_SECRET_KEY=your_thirdweb_secret_key_here

# Thirdweb server wallet address
# This is the wallet that will facilitate payments
THIRDWEB_SERVER_WALLET_ADDRESS=your_server_wallet_address_here

# Platform wallet address (optional)
# This is where platform fees would be sent if implemented
PLATFORM_WALLET_ADDRESS=your_platform_wallet_address_here
